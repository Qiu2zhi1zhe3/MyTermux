#!/system/bin/sh
a=(cache code_cache anr tombstones MiPushLog log logs)
for b in ${a[@]}; do
for i in $(find /data/ -type d -iname ${b}); do
find ${i} -type f -delete > /dev/null 2>&1
done
done
find /storage/emulated/0/ -type d -empty -delete > /dev/null 2>&1
rm -fr /data/dalvik-cache
rm -fr /data/cache
rm -R /data/magisk_backup_*
#rm -fr /data/system/package_cache
#rm -R /data/system/package_cache
mount -o remount,rw /
echo $(date +'%T %m/%d') "clean up successfully"